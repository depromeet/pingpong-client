{
  "hosting": [
    {
      "target": "main",
      "public": "dist/main",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "destination": "/posts/[id]/index.html",
          "regex": "^/posts/([^/]+?)(?:/)?$"
        },
        {
          "source": "/",
          "destination": "//index.html"
        },
        {
          "source": "/dev",
          "destination": "/dev/index.html"
        },
        {
          "source": "/main",
          "destination": "/main/index.html"
        },
        {
          "source": "/nickname",
          "destination": "/nickname/index.html"
        },
        {
          "source": "/playground",
          "destination": "/playground/index.html"
        },
        {
          "source": "/profile",
          "destination": "/profile/index.html"
        },
        {
          "source": "/profile/edit",
          "destination": "/profile/edit/index.html"
        },
        {
          "source": "/profile/edit/givenTalents",
          "destination": "/profile/edit/givenTalents/index.html"
        },
        {
          "source": "/profile/edit/takenTalents",
          "destination": "/profile/edit/takenTalents/index.html"
        },
        {
          "source": "/setting",
          "destination": "/setting/index.html"
        },
        {
          "source": "/setting/alarm",
          "destination": "/setting/alarm/index.html"
        },
        {
          "source": "/setting/license",
          "destination": "/setting/license/index.html"
        },
        {
          "source": "/setting/privacy",
          "destination": "/setting/privacy/index.html"
